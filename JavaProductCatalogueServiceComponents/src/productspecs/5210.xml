            <attributeName>DontCrossSellOnBaseComponentStatuses</attributeName>
            <value>1012</value>
        </productAttributes>
        <!-- Any attribute starting with VAS_ must be cross-sold. In this instance, the logic is complex so we tell the engine to run a specific class method to determine whether the component must be cross-sold -->
