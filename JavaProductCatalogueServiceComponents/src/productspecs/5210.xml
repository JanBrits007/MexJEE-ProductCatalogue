            <attributeType>string</attributeType>
            <attributeName>VAS_1376</attributeName>
            <value>classname=za.co.nb.offermanagement.crosssell.SalarySwitchingCrossSellHandler</value>
        </productAttributes>
        <!-- This is a basic cross-sell definition to say cross sell as optional and not defaulted to taken up -->
        <productAttributes>
            <attributeType>string</attributeType>
            <attributeName>VAS_1380</attributeName>
            <value>Optionality=Optional~IsTakenUp=false</value>
        </productAttributes>
        <productAttributes>
