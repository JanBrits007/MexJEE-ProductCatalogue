   <!--Initialization Handler-->
   <productAttributeGroup>
      <attributeGroupName>CaseConfiguration</attributeGroupName>
      <productAttributes>
         <attributeType>string</attributeType>
         <attributeName>ArrangementBundleCaseHandler</attributeName>
         <value>classname=za.co.nb.onboarding.product.initialisebusinesscase.handlers.retail.InitialiseBusinessCaseHandlerRetailTP</value>
      </productAttributes>
   </productAttributeGroup>

   <!--Substitution and Routing Rules-->
   <productAttributeGroup>
      <attributeGroupName>SubstitutionRulesETE</attributeGroupName>
   </productAttributeGroup>
   <productAttributeGroup>
      <attributeGroupName>SubstitutionRulesQA</attributeGroupName>
   </productAttributeGroup>
   <productAttributeGroup>
      <attributeGroupName>SubstitutionRulesPROD</attributeGroupName>
   </productAttributeGroup>


      <!--ETE Substitution Rules-->
   <productAttributeGroup>
      <attributeGroupName>InitialisationSubstitutionRulesETE</attributeGroupName>
      <productAttributes>
         <attributeName>DefaultSubstitution</attributeName>
         <value>DefaultSubstitution</value>
      </productAttributes>
      <productAttributes>
         <attributeName>SubstituteForProductID</attributeName>
         <value>Transactional_Retail_TP20_4802</value>
      </productAttributes>
   </productAttributeGroup>

   <!--QA Substitution Rules-->
   <productAttributeGroup>
      <attributeGroupName>InitialisationSubstitutionRulesQA</attributeGroupName>
      <productAttributes>
         <attributeName>DefaultSubstitution</attributeName>
         <value>DefaultSubstitution</value>
      </productAttributes>
      <productAttributes>
         <attributeName>SubstituteForProductID</attributeName>
         <value>Transactional_Retail_TP20_4802</value>
      </productAttributes>
   </productAttributeGroup>

   <!--Prepare Applicant Configuration-->
   <productAttributeGroup>
      <attributeGroupName>clientEvidenceRequirement_PrepareApplicant</attributeGroupName>
      <productAttributes>
         <attributeType>Evidence</attributeType>
         <attributeName>ProofOfIdentificationClause</attributeName>
         <isComparable>false</isComparable>
         <valueUnit>bool</valueUnit>
      </productAttributes>
   </productAttributeGroup>
   <productAttributeGroup>
      <attributeGroupName>EligibilityRules</attributeGroupName>
      <productAttributes>
         <attributeType>EligibilityRule</attributeType>
         <attributeName>Age</attributeName>
         <isComparable>false</isComparable>
         <minValue>16</minValue>
         <maxValue>999</maxValue>
         <valueUnit>year</valueUnit>
         <description>be 16 or older</description>
      </productAttributes>
   </productAttributeGroup>
   <productAttributeGroup>
      <attributeGroupName>RequiredCompliance_PrepareApplicant</attributeGroupName>
      <productAttributes>
         <attributeType>Compliance</attributeType>
         <attributeName>PersonalInformationProcessingConsent</attributeName>
      </productAttributes>
      <productAttributes>
         <attributeType>Compliance</attributeType>
         <attributeName>BureauCheckConsent</attributeName>
      </productAttributes>
   </productAttributeGroup>
   <productAttributeGroup>
      <attributeGroupName>BusinessProcessExecutionConfiguration</attributeGroupName>
      <productAttributes>
         <attributeType>int</attributeType>
         <attributeName>ApplicationCaptureExecutionTimeMinutes</attributeName>
         <value>5</value>
      </productAttributes>

   </productAttributeGroup>

</product>
