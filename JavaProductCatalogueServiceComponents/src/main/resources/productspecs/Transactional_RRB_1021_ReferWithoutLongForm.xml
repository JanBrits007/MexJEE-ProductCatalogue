<product inheritFromFiles="Transactional_RRB_1021" xmlns="http://contracts.it.nednet.co.za/services/ent/productandservicedevelopment/ChannelProductCatalogue/v1">
	<productIdentifier>1021</productIdentifier>
	<name>Professional Bundle</name>
	<shortDescription>(Transactional_RRB_1021_ReferWithoutLongForm)</shortDescription>

	<!--Activate Product Gather Product Details-->
	<productAttributeGroup inheritFromFiles="BaseProduct_RRB_ReferWithoutLongForm">
		<attributeGroupName>productInformationRequirement_ActivateProducts</attributeGroupName>
	</productAttributeGroup>

	<!--FinalizeFrontOfficeInteraction-->
	<productAttributeGroup inheritFromFiles="BaseProduct_RRB_ReferWithoutLongForm">
		<attributeGroupName>productInformationRequirement_FinalizeFrontOfficeInteraction</attributeGroupName>
	</productAttributeGroup>

	<!--Case and Process Configuration-->
	<productAttributeGroup inheritFromFiles="BaseProduct_RRB_ReferWithoutLongForm">
		<attributeGroupName>IntentSequenceConfiguration</attributeGroupName>
	</productAttributeGroup>

	<!--Overdraft feature -->
	<features>
		<featureIdentifier>1014</featureIdentifier>
		<name>Overdraft</name>
		<mandatory>true</mandatory>
		<preSelected>true</preSelected>

		<!--ActivateProductsGatherProductDetails-->
		<featureAttributeGroup action="overwrite">
			<attributeGroupName>productInformationRequirement_ActivateProducts</attributeGroupName>
			<featureAttributes>
				<attributeType>productInformationRequirement</attributeType>
				<attributeName>KeepingInTouchInformationV2|sideStepperDisplayName=Keep in touch</attributeName>
			</featureAttributes>
		</featureAttributeGroup>

		<!-- Intent sequence mapping -->
		<featureAttributeGroup action="overwrite">
			<attributeGroupName>IntentSequenceConfiguration</attributeGroupName>
			<featureAttributes>
				<attributeType>string</attributeType>
				<attributeName>frontOfficeIntentSequence</attributeName>
				<value>VerifyIdentity|GenerateOfferVerifyEligibility|GenerateOfferGatherProductDetails|GenerateOfferGatherClientDetails|FinancialDetails|GenerateOffer|ActivateProductsGatherClientDetails|ActivateProductsGatherProductDetails|ActivateProductsGatherEvidence|FinalizeFrontOfficeInteraction</value>
			</featureAttributes>
			<featureAttributes>
				<attributeType>string</attributeType>
				<attributeName>breadcrumbSteps</attributeName>
				<value>Authenticate|Product Setup|Credit Info|About You|Customise|Finishing up</value>
			</featureAttributes>
			<featureAttributes>
				<attributeType>string</attributeType>
				<attributeName>GNBAIntentToActivityMapping</attributeName>
				<value>VerifyIdentity=Authenticate|GenerateOfferVerifyEligibility=Authenticate|GenerateOfferGatherProductDetails=Product Setup|GenerateOfferGatherClientDetails=Product Setup|FinancialDetails=Credit Info|GenerateOffer=Credit Info|ActivateProductsGatherClientDetails=About you|ActivateProductsGatherProductDetails=Customise|ActivateProductsGatherEvidence=Finishing up|FinalizeFrontOfficeInteraction=Finishing up</value>
			</featureAttributes>
		</featureAttributeGroup>

	</features>

</product>
